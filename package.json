{
  "name": "sentryfrogg-mcp",
  "version": "6.4.0",
  "description": "Enterprise MCP server for PostgreSQL, SSH and HTTP operations under the SentryFrogg brand",
  "main": "dist/sentryfrogg_server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:test": "tsc -p tsconfig.test.json",
    "ensure:build": "node -e \"const fs=require('node:fs'); const path=require('node:path'); const { spawnSync }=require('node:child_process'); const dist=path.join(process.cwd(),'dist','sentryfrogg_server.js'); if (fs.existsSync(dist)) process.exit(0); const res=spawnSync('npm',['run','build'],{stdio:'inherit'}); process.exit(res.status===null?1:res.status);\"",
    "prestart": "npm run ensure:build",
    "start": "node dist/sentryfrogg_server.js",
    "predev": "npm run ensure:build",
    "dev": "node dist/sentryfrogg_server.js",
    "check": "tsc -p tsconfig.json --noEmit",
    "test": "npm run build:test && node --test dist/tests/index.js",
    "smoke": "npm run build:test && node dist/integration/smoke.js",
    "prepack": "npm run build",
    "postinstall": "npm run ensure:build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "postgresql",
    "postgres",
    "database",
    "sql",
    "api",
    "rest-api",
    "ssh",
    "cursor",
    "claude",
    "ai",
    "artificial-intelligence",
    "database-management",
    "server-management",
    "migration",
    "schema",
    "analytics",
    "security",
    "validation"
  ],
  "author": "AmirTlinov",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "ajv": "^8.17.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.17.1",
    "ssh2": "^1.15.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "@types/pg": "^8.11.6",
    "@types/ssh2": "^1.15.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmirTlinov/sentryfrogg-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/AmirTlinov/sentryfrogg-mcp/issues"
  },
  "homepage": "https://github.com/AmirTlinov/sentryfrogg-mcp#readme",
  "files": [
    "sentryfrogg_server.ts",
    "dist/",
    "src/",
    "scripts/",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "profiles.example.json",
    "projects.example.json",
    "runbooks.json",
    "capabilities.json"
  ],
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "mcp_version": "1.0",
    "supported_protocols": [
      "mcp",
      "postgresql",
      "ssh",
      "http"
    ]
  }
}
